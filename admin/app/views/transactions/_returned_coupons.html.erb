<h2>Your returned coupons</h2>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Creator address</th>
      <th>Payload</th>
      <th>Generate spent transaction</th>
    </tr>
  </thead>

  <tbody>
    <% @coupons_returned.each do |coupon| %>
      <tr>
        <td><%= coupon['creatorAddress'] %></td>
        <td><%= coupon['payload'] %></td>
        <td><%= link_to 'Delete', '#', class: "btn btn-warning"%></td>
      </tr>
    <% end %>
  </tbody>
</table>
