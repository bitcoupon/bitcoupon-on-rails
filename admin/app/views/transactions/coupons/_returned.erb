<h2>Your returned coupons</h2>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th width="50%">TITLE</th>
      <th width="50%">MARK AS SPENT</th>
    </tr>
  </thead>

  <tbody>
    <% @coupons_returned.each do |coupon| %>
      <tr>
        <td><%= JSON.parse(coupon['payload'])["title"] %></td>
        <td><%= render 'transactions/forms/delete', creator_address: coupon['creatorAddress'], payload: coupon['payload'], private_key: 'return', submit_name: 'Accept' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
