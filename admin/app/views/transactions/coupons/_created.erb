<h2>Your coupons</h2>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th width="25%">TITLE</th>
      <th width="55%">SEND COUPON TO MOBILE ID</th>
      <th width="20%">CANCEL</th>
    </tr>
  </thead>

  <tbody>
    <% @coupons.each do |coupon| %>
      <tr>
        <td class="break"><%= JSON.parse(coupon['payload'])['title'] %></td>
        <td><%= render 'transactions/forms/issue', creator_address: coupon['creatorAddress'], payload: coupon['payload'] %></td>
        <td><%= render 'transactions/forms/delete', creator_address: coupon['creatorAddress'], payload: coupon['payload'], private_key: 'create', submit_name: 'Cancel' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
