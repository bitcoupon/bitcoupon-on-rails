<h2>Your circulating coupons</h2>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Creator</th>
      <th>Title</th>
      <th>Owner</th>
    </tr>
  </thead>

  <tbody>
    <% @coupons_circulating.each do |coupon| %>
      <tr>
        <td><%= translate_address coupon['coupon']['creatorAddress'] %></td>
        <td><%= coupon['coupon']['payload'] %></td>
        <td><%= translate_address coupon['ownerAddress'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
