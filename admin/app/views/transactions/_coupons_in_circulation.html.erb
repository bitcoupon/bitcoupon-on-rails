<h2>Your Coupons in circulation</h2>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Owner address</th>
      <th>Payload</th>
      <th>Cancel</th>
    </tr>
  </thead>

  <tbody>
    <% @coupons.each do |coupon| %>
      <tr>
        <td><%= coupon['creatorAddress'] %></td>
        <td><%= coupon['payload'] %></td>
        <td><%= link_to '??Cancel??', '#', class: "btn btn-danger"%></td>
      </tr>
    <% end %>
  </tbody>
</table>
